@model Movies.DataModel.Studio

<dl>
    <dt>Studio name:</dt>
    <dd>@Model.StudioName</dd>
    <dt>Studio address:</dt>
    <dd>@Model.StudioAddress</dd>
    <dt>Movies: </dt>

    <dd>
        @for (int i = 0; i < Model.Movies.Count; i++)
        {
            <div>@string.Format("\"{0}\" by {1}", Model.Movies.ElementAt(i).Title, Model.Movies.ElementAt(i).Director)</div>
        }
    </dd>
</dl>